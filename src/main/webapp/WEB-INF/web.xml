<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ArtistControllerServlet</servlet-name>
		<servlet-class>com.github.ekroes.crowflies.ArtistControllerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AlbumControllerServlet</servlet-name>
		<servlet-class>com.github.ekroes.crowflies.AlbumControllerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserControllerServlet</servlet-name>
		<servlet-class>com.github.ekroes.crowflies.UserControllerServlet</servlet-class>
	</servlet>



	<servlet>
		<description>
		</description>
		<display-name>LoginControllerServlet</display-name>
		<servlet-name>LoginControllerServlet</servlet-name>
		<servlet-class>com.github.ekroes.crowflies.LoginControllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ArtistControllerServlet</servlet-name>
		<url-pattern>/artist</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AlbumControllerServlet</servlet-name>
		<url-pattern>/album</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserControllerServlet</servlet-name>
		<url-pattern>/users</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginControllerServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

</web-app>
